# Polarion instance base URL (no trailing slash)
POLARION_URL=https://your-polarion-instance.com

# Bearer token for Polarion REST API
POLARION_TOKEN=your-personal-access-token

# Default Polarion project ID
POLARION_PROJECT_ID=your-project-id

# Set to false to disable SSL verification (not recommended for production)
POLARION_VERIFY_SSL=true
